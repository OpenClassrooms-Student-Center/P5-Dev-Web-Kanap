class Kanap {
    constructor(data) {
        this._colors = data.colors
        this._id = data._id
        this._name = data.name
        this._imageUrl = data.imageUrl
        this._altTxt = data.altTxt
        this._price = data.price
        this._description = data.description
    }
    get colors() {
        return this_colors
    }
    get id() {
        return this._id
    }
    get name() {
        return this._name
    }
    get picture() {

        return this._imageUrl
    }
    get altTxt() {
        return this._altTxt
    }
    get price() {
        return this.price
    }
    get description() {
        return this._description
    }

}